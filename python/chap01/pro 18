def init (data, count) :
    for i in range (count) :
        print("노드 #1 데이터 : ")
        data[i] = input()
    print ("리스트의 내용: ")
    
    return data

def main() :

    count = int(input("노드의 개수를 입력하세요: "))
    data = [None] * count       # count 개수만큼 None으로 초기화된 리스트 생성
    print(init(data, count))

    return 0

if __name__ == "__main__":
    main()